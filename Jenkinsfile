pipeline{
agent any
stages{
 stage('Printing a message '){
    steps{
        echo"hello maven and other world"
        sh 'mvn --version'
    }
    }
     stage('stge 2'){
    steps{
        echo"printing 2nd stage"
    }
    }
     stage('this is stage 3'){
    steps{
        echo"printing 3rd stage"
    }
}
}
}
